{
  "name": "app",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.6",
    "@types/react-router": "^5.1.20",
    "esbuild": "^0.19.2",
    "immer": "^10.0.2",
    "prop-types": "^15.8.1",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "typescript": "5.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@yarnpkg/sdks": "^3.0.0-rc.50",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "prettier"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  },
  "eslintIgnore": [
    ".yarn/*",
    "app/assets/*"
  ],
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --minify --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl",
    "build-dev": "esbuild app/javascript/*.* --bundle --watch --sourcemap --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl"
  }
}
