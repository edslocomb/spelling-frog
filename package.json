{
  "name": "app",
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@types/react-router": "^5.1.20",
    "esbuild": "^0.17.18",
    "prop-types": "^15.8.1",
    "radash": "^10.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0",
    "typescript": "4.9"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "root": true
  },
  "eslintIgnore": [
    ".yarn/*",
    "app/assets/*"
  ],
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --minify --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl",
    "build-dev": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl"
  }
}
