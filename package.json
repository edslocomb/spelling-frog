{
  "name": "app",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@types/react-router": "^5.1.20",
    "esbuild": "^0.18.13",
    "immer": "^10.0.2",
    "prop-types": "^15.8.1",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "typescript": "4.9.5",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^3.0.0"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "prettier"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  },
  "eslintIgnore": [
    ".yarn/*",
    "app/assets/*"
  ],
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --minify --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl",
    "build-dev": "esbuild app/javascript/*.* --bundle --watch --sourcemap --outdir=app/assets/builds --public-path=assets --loader:.woff=dataurl --loader:.woff2=dataurl"
  }
}
